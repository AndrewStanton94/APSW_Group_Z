<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <navigation-rule>
        <from-view-id>/view/submit/SubmitConfigurationPage.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toSubmitConfEdit</from-outcome>
            <to-view-id>/view/submit/SubmitConfigurationEditPage.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/view/submit/SubmitProject.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toSubmitProjectEdit</from-outcome>
            <to-view-id>/view/submit/SubmitProjectEdit.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
</faces-config>
<?xml version='1.0' encoding='UTF-8'?>

<faces-config version="2.0"
    xmlns="http://java.sun.com/xml/ns/javaee" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

    <navigation-rule>
        <from-view-id>/view/allocation/projectDetails.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toMarkingForm</from-outcome>
            <to-view-id>/view/marking/markingForm.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    
    <navigation-rule>
        <from-view-id>/view/marking/markingForm.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>toProjectAllocatedDetails</from-outcome>
            <to-view-id>/view/allocation/projectDetails.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>    
</faces-config>
